  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "d3": "^5.9.2",
    "eslint": "^5.16.0",
    "inspector": "^0.5.0",
    "nunjucks": "^3.1.3",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.14.3"
  },
  "config": {
